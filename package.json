{
  "name": "monitor",
  "version": "0.0.1",
  "description": "Monitoring tool for my personal server",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=dev concurrently \"npm run server\" \"npm run client\"",
    "server": "NODE_ENV=debug nodemon server.js",
    "server-prod": "NODE_ENV=prod forever --id monitor start server.js",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "generate-logs": "node scripts/generate_logs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cooya/Monitor.git"
  },
  "author": "Coya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Cooya/Monitor/issues"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10",
    "react-scripts": "^2.1.8"
  },
  "dependencies": {
    "@coya/time": "^0.1.1",
    "bulma": "^0.7.4",
    "chroma-js": "^2.0.3",
    "express": "^4.16.4",
    "mongodb": "^3.2.3",
    "random-textblock": "^1.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-select": "^2.4.2",
    "winston": "^3.2.1",
    "winston-mongodb": "^5.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:8080"
}
